<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bhanuka.braintraininggame">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_math_hero_game_icon"
        android:label="@string/appName"
        android:supportsRtl="true"
        android:theme="@style/LoginTheme">
        <activity android:name=".ui.activity.LoginGameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <activity android:name=".ui.activity.MathNinjaGameActivity">
        </activity>
        <activity android:name=".ui.activity.MainMenuActivity"></activity>
        <activity android:name=".ui.activity.ContinueGameActivity"></activity>
    </application>

</manifest>